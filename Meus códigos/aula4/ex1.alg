algoritmo "Idade de Creusa01"
var
 anoAtual, anoDeNascimento, idade: inteiro
inicio
      escreva("Qual ano estamos?: ")
      leia (anoAtual)
      
      escreva("Qual ano eu nasci?: ")
      leia(anoDeNascimento)
      
      idade <- anoAtual - anoDeNascimento
      
      escreva ("Minhe idade é: ", idade)
fimalgoritmo
